    <AssemblyName>Dnc.Api.Throttle</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/kulend/Dnc.Api.Throttle</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/kulend/Dnc.Api.Throttle/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/kulend/Dnc.Api.Throttle</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Authors>kulend@qq.com</Authors>
    <Company>www.kulend.com</Company>
    <Description>适用于.Net Core WebApi接口限流框架</Description>
    <Version>1.0.1</Version>
